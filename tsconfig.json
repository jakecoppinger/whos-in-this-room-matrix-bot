{
  "lib": [ "es6" ],
  "compilerOptions": {
      "sourceMap": true,
      "moduleResolution": "node",
      "lib": ["dom", "es6"],
      "target": "ES5",
      "noImplicitAny": true,
      "pretty": true,
      "sourceRoot": "./src/",
      "rootDir": "./src/",
      "outDir": "dist/",
      "noUnusedParameters": true,
      "incremental": true,
      "resolveJsonModule": true,
      "strict": true,
      "alwaysStrict": false,
      "strictNullChecks": true,

  },
  "files": [
      "src/index.ts"
  ]
}
